{
  "name": "@alauda/chart",
  "version": "0.0.9-beta",
  "type": "module",
  "description": "alauda chart",
  "repository": "git+https://gitlab-ce.alauda.cn/frontend/alauda-chart",
  "author": "alauda",
  "license": "ISC",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    "types": "./lib/index.d.ts",
    "import": "./lib/index.d.ts",
    "require": "./lib.index.d.ts"
  },
  "fesm2020": "lib/index.es2020.mjs",
  "fesm2015": "lib/index.es2015.mjs",
  "unpkg": "lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "keywords": [
    "alauda",
    "chart",
    "d3"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build-storybook": "build-storybook",
    "build:r": "r -f cjs,es2015,es2020",
    "build:scss": "sass src/theme/default.scss ./lib/default.css",
    "build:ts": "tsc -p tsconfig.lib.json",
    "dev": "start-storybook -p 6006",
    "lint": "run-p lint:*",
    "lint:es": "eslint . -f friendly",
    "lint:style": "stylelint . --cache",
    "lint:tsc": "tsc --noEmit",
    "postinstall": "./simple-git-hooks && yarn-deduplicate --strategy fewer || exit 0",
    "prerelease": "yarn build",
    "release": "changeset publish"
  },
  "peerDependencies": {
    "d3": "^7.1.1",
    "lodash": "^4.17.21"
  },
  "dependencies": {
    "d3": "^7.6.1"
  },
  "devDependencies": {
    "@1stg/app-config": "^6.1.4",
    "@1stg/lib-config": "^9.0.1",
    "@babel/core": "^7.18.6",
    "@changesets/changelog-github": "^0.4.5",
    "@changesets/cli": "^2.23.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/cli": "^6.5.9",
    "@storybook/html": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/preset-scss": "^1.0.3",
    "@types/d3": "^7.4.0",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-loader": "^8.2.5",
    "eslint-import-resolver-typescript": "^3.2.7",
    "eslint-plugin-import": "^2.26.0",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "tslib": "^2.4.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1",
    "undertaker-forward-reference": "^2.0.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "resolutions": {
    "prettier": "^2.4.1"
  },
  "commitlint": {
    "extends": "@1stg"
  },
  "eslintIgnore": [
    "dist",
    "!/.storybook",
    "!/.*.js"
  ],
  "markuplint": {
    "extends": "@1stg/markuplint-config"
  },
  "prettier": "@1stg/prettier-config/semi"
}
